    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "vite": "^2.0.1"
  },
  "peerDependencies": {
    "react": ">=0.14.x",
    "react-dom": ">=0.14.x"
